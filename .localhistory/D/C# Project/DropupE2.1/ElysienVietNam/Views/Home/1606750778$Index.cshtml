@using Data;
@using Data.Providers;
@{
    ViewBag.Title = "Elysien - Make Up Your Beauty";
    Layout = "~/Views/Shared/_Layout.cshtml";

    string urlCMS = System.Configuration.ConfigurationManager.AppSettings["urlCMS"];
    string urlSite = System.Configuration.ConfigurationManager.AppSettings["urlSite"];

    var list_slide = new StoreSlideProvider().getIsShowing();
    var banner = new StoreBannerProvider().getbyId(1);

    var list_page_main_content = new PageProvider().getAll().Where(p => p.page_id == 6 || p.page_id == 7 || p.page_id == 8).ToList();
    var list_category_parent = new CategoryProvider().getListParentCategory().Where(c => c.type_of_category_id == 1).ToList();

    var list_blog_newest = new BlogProvider().getIsShowing().OrderByDescending(b => b.show_datetime).Take(5).ToList();
    var list_cs = new StoreCustomerSayProvider().getAll();
    var logo_header = new StoreLogoProvider().getLogoHeader();

    ViewBag.Keywords = "elysien, mỹ phẩm, spa, đào tạo spa, oem, odm, máy móc dụng cụ spa";
    ViewBag.Description = "Thương hiệu mỹ phẩm, chăm sóc sắc đẹp Hàn Quốc tại Việt Nam.";
    ViewBag.Canonical = urlSite;
    ViewBag.Alternate = urlSite;
    ViewBag.Url = urlSite;
    ViewBag.Image = urlCMS + "/" + logo_header.image;
}

@if (list_slide.Count > 0)
{
    <!-- Slider Arae Start -->
    <div class="slider-area">
        <div class="slider-active-3 owl-carousel slider-hm8 owl-dot-style">
            @foreach (var slide in list_slide)
            {
                <!-- Slider Single Item Start -->
                <div class="slider-height-6 d-flex align-items-start justify-content-start bg-img" style="background-image: url(@urlCMS/@slide.image);">
                    <div class="container">
                        <div class="slider-content-1 slider-animated-1 text-left">
                            <span class="animated">
                                @slide.child_text_1
                            </span>
                            <h1 class="animated">
                                @slide.main_text
                            </h1>
                            <p class="animated">
                                @slide.child_text_2
                            </p>
                            <a href="@slide.link_url" class="shop-btn animated">Xem Chi Tiết</a>
                        </div>
                    </div>
                </div>
                <!-- Slider Single Item End -->
            }

        </div>
    </div>
}


<!-- Category Area Start -->
<section class="categorie-area mt-res-md-30px">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <!-- Section Title -->
                <div class="section-title section-title-center mt-res-sx-30px mt-res-md-30px">
                    <h2>Lĩnh vực kinh doanh</h2>
                    <p>Các lĩnh vực mà Elysien đang tập trung và tầm nhìn thương hiệu</p>
                </div>
                <!-- Section Title -->
            </div>
        </div>
        <!-- Category Slider Start -->
        <div class="row">
            <!-- Single item -->
            @if (list_page_main_content.Count > 0)
            {
                foreach (var page in list_page_main_content)
                {
                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                        <div class="category-item">
                            <div class="category-list mb-30px">
                                <div class="category-thumb">
                                    <a href="shop-4-column.html">
                                        <img src="@urlCMS/@page.page_image" alt="@page.page_image_alt" />
                                    </a>
                                </div>
                                <div class="desc-listcategoreis">
                                    <div class="name_categories">
                                        <h4>@page.page_title</h4>
                                    </div>
                                    <a href="/pages/@page.page_seo_alias">Chi Tiết <i class="ion-android-arrow-dropright-circle"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
            @if (list_category_parent.Count > 0)
            {
                foreach (var category in list_category_parent)
                {
                    <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12">
                        <div class="category-item">
                            <div class="category-list mb-30px">
                                <div class="category-thumb">
                                    <a href="shop-4-column.html">
                                        <img src="@urlCMS/@category.image" alt="@category.alt_image" />
                                    </a>
                                </div>
                                <div class="desc-listcategoreis">
                                    <div class="name_categories">
                                        <h4>@category.category_name</h4>
                                    </div>
                                    <a href="/collections/@category.seo_alias">Chi Tiết <i class="ion-android-arrow-dropright-circle"></i></a>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
            @*<div>
                    <img src="~/Assets/images/common/1-3.png" style="position: absolute; left: 70%; width: 10%; margin-top: -65px;" />
                    <img src="~/Assets/images/common/1-2.png" style="position: absolute; left: 75%; width: 10%;margin-top: 180px;" />
                </div>*@
        </div>
    </div>
</section>
<!-- Category Area End  -->

<section class="about-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 mb-res-sm-50px">
                <div class="about-left-image">
                    <img src="../Assets/images/banner-image/banner-au.jpg" class="img-responsive" />
                </div>
            </div>
            <div class="col-lg-6">
                <div class="about-content">
                    <div class="about-title">
                        <h2>Elysien Việt Nam</h2>
                    </div>
                    <p class="mb-30px">
                        “Được thành lập từ năm 2002 tại Hàn Quốc, hơn 20 năm kinh nghiệm OEM/ODM sản xuất mĩ phẩm, máy móc, Công ty Raparay (Elysien) được công nhận bởi các Hiệp hội da liễu khó tính nhất Hàn Quốc.
                    </p>
                    <p class="mb-30px">
                        Hướng đến trở thành đơn vị tiên phong kiến tạo nhận thức đúng đắn về làm đẹp an toàn, hiệu quả đến phụ nữ Việt Nam bằng các lĩnh vực chúng tôi có: cơ sở Spa, sản phẩm chuyên nghiệp, máy móc, OEM, đào tạo, setup Spa.
                    </p>
                    <p class="mb-30px">
                        Sản phẩm của chúng tôi đã nhận được nhiều lời khen ở thị trường Châu Âu, và hiện tại, chúng tôi đang xúc tiến vào thị trường Việt Nam để các bạn có thể trực tiếp trải nghiệm tính ưu việt dịch vụ của chúng tôi.”
                    </p>
                    <div padding-top: 30px;">
                        <a href="/pages/ve-elysien" class="shop-btn animated">Xem Chi Tiết</a>
                        <a href="/pages/ve-elysien" class="shop-btn animated">Tải Profile</a>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
